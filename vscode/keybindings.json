// %appdata%/Code/User
[
	// -- unbind defaults
	{ "key": "ctrl+m", "command": "-workbench.action.toggleTabsVisibility" },
	{ "key": "ctrl+up", "command": "-scrollLineUp" },
	{ "key": "ctrl+down", "command": "-scrollLineDown" },
	{ "key": "alt+pageup", "command": "-scrollPageUp" },
	{ "key": "alt+pagedown", "command": "-scrollPageDown" },
	{ "key": "ctrl+-", "command": "-workbench.action.zoomOut" },
	{ "key": "ctrl+=", "command": "-workbench.action.zoomIn" },
	{ "key": "ctrl+0", "command": "-workbench.action.focusSideBar" },
	{ "key": "ctrl+1", "command": "-workbench.action.focusFirstEditorGroup" },
	{ "key": "ctrl+2", "command": "-workbench.action.focusSecondEditorGroup" },
	{ "key": "ctrl+3", "command": "-workbench.action.focusThirdEditorGroup" },
	{ "key": "ctrl+6", "command": "-workbench.action.focusSixthEditorGroup" },
	{ "key": "ctrl+7", "command": "-workbench.action.focusSeventhEditorGroup" },
	{ "key": "ctrl+4", "command": "-workbench.action.focusFourthEditorGroup" },
	{ "key": "ctrl+5", "command": "-workbench.action.focusFifthEditorGroup" },
	{ "key": "ctrl+8", "command": "-workbench.action.focusEighthEditorGroup" },
	{ "key": "ctrl+9", "command": "-workbench.action.lastEditorInGroup" },
	{ "key": "ctrl+numpad0", "command": "-workbench.action.zoomReset" },
	{ "key": "ctrl+numpad_add", "command": "-workbench.action.zoomIn" },
	{ "key": "ctrl+numpad_subtract", "command": "-workbench.action.zoomOut" },
	{ "key": "ctrl+shift+=", "command": "-workbench.action.zoomIn" },
	{ "key": "ctrl+shift+-", "command": "-workbench.action.zoomOut" },
	{ "key": "ctrl+f12", "command": "-editor.action.revealDefinition" },
	{ "key": "ctrl+f12", "command": "-editor.action.goToImplementation" },
	{ "key": "ctrl+home", "command": "-cursorTop" },
	{ "key": "ctrl+end", "command": "-cursorBottom" },
	{ "key": "ctrl+shift+home", "command": "-cursorTopSelect" },
	{ "key": "ctrl+shift+end", "command": "-cursorBottomSelect" },
	{ "key": "ctrl+pageup", "command": "-workbench.action.previousEditor" },
	{ "key": "ctrl+pagedown", "command": "-workbench.action.nextEditor" },
	{ "key": "ctrl+shift+pageup", "command": "-workbench.action.moveEditorLeftInGroup" },
	{ "key": "ctrl+shift+pagedown", "command": "-workbench.action.moveEditorRightInGroup" },
	{ "key": "ctrl+f9", "command": "-workbench.action.chat.nextFileTree" },
	{ "key": "ctrl+shift+k", "command": "-editor.action.deleteLines" },
	{ "key": "ctrl+alt+shift+down", "command": "-cursorColumnSelectDown" },
	{ "key": "ctrl+alt+shift+up", "command": "-cursorColumnSelectUp" },
	{ "key": "ctrl+shift+m", "command": "-workbench.actions.view.problems" },
	{ "key": "ctrl+r", "command": "-workbench.action.openRecent" },
	{ "key": "alt+f8", "command": "-editor.action.marker.next", "when": "editorFocus" },
	{ "key": "shift+alt+f8", "command": "-editor.action.marker.prev", "when": "editorFocus" },
	{ "key": "f8", "command": "-editor.action.marker.nextInFiles", "when": "editorFocus" },
	{ "key": "shift+f8", "command": "-editor.action.marker.prevInFiles", "when": "editorFocus" },
	{ "key": "f4", "command": "-references-view.next", "when": "reference-list.hasResult && references-view.canNavigate" },
	{ "key": "shift+f4", "command": "-references-view.prev", "when": "reference-list.hasResult && references-view.canNavigate" },
	{ "key": "f4", "command": "-search.action.focusNextSearchResult", "when": "hasSearchResult || inSearchEditor" },
	{ "key": "shift+f4", "command": "-search.action.focusPreviousSearchResult", "when": "hasSearchResult || inSearchEditor" },
	{ "key": "f4", "command": "-goToNextReference", "when": "inReferenceSearchEditor || referenceSearchVisible" },
	{ "key": "shift+f4", "command": "-goToPreviousReference", "when": "inReferenceSearchEditor || referenceSearchVisible" },
	{ "key": "ctrl+k ctrl+s", "command": "-workbench.action.openGlobalKeybindings" },
	{ "key": "alt+left", "command": "-workbench.action.navigateBack", "when": "canNavigateBack" },
	{ "key": "alt+right", "command": "-workbench.action.navigateForward", "when": "canNavigateForward" },
	{ "key": "ctrl+alt+right", "command": "-workbench.action.moveEditorToNextGroup" },
	{ "key": "ctrl+alt+left", "command": "-workbench.action.moveEditorToPreviousGroup" },
	// -- utilities
	{
		"key": "ctrl+shift+r",
		"command": "workbench.view.extension.references-view",
	},
	{
		"key": "ctrl+alt+t",
		"command": "workbench.action.hideEditorTabs",
		"when": "editorTabsVisible",
	},
	{
		"key": "ctrl+alt+t",
		"command": "workbench.action.showMultipleEditorTabs",
		"when": "!editorTabsVisible",
	},
	{
		"key": "shift+f11",
		"command": "workbench.action.toggleZenMode",
	},
	{
		"key": "ctrl+-",
		"command": "editor.action.fontZoomOut",
	},
	{
		"key": "ctrl+=",
		"command": "editor.action.fontZoomIn",
	},
	{
		"key": "ctrl+0",
		"command": "editor.action.fontZoomReset",
	},
	{
		"key": "shift+alt+p",
		"command": "workbench.action.openRecent",
	},
	// -- RMB
	{
		"key": "ctrl+,",
		"command": "editor.action.showContextMenu",
		"when": "textInputFocus",
	},
	// -- scroll editor
	{
		"key": "ctrl+up",
		"command": "editorScroll",
		"when": "editorTextFocus",
		"args": {"to": "up", "by": "line", "revealCursor": true},
	},
	{
		"key": "ctrl+down",
		"command": "editorScroll",
		"when": "editorTextFocus",
		"args": {"to": "down", "by": "line", "revealCursor": true},
	},
	{
		"key": "alt+pageup",
		"command": "editorScroll",
		"when": "editorTextFocus",
		"args": {"to": "up", "by": "page", "revealCursor": true},
	},
	{
		"key": "alt+pagedown",
		"command": "editorScroll",
		"when": "editorTextFocus",
		"args": {"to": "down", "by": "page", "revealCursor": true},
	},
	// -- navigate
	{
		"key": "ctrl+home",
		"command": "workbench.action.previousEditor",
	},
	{
		"key": "ctrl+end",
		"command": "workbench.action.nextEditor",
	},
	{
		"key": "ctrl+shift+home",
		"command": "workbench.action.moveEditorLeftInGroup",
	},
	{
		"key": "ctrl+shift+end",
		"command": "workbench.action.moveEditorRightInGroup",
	},
	{
		"key": "ctrl+pageup",
		"command": "cursorTop",
	},
	{
		"key": "ctrl+pagedown",
		"command": "cursorBottom",
	},
	{
		"key": "ctrl+shift+pageup",
		"command": "cursorTopSelect",
	},
	{
		"key": "ctrl+shift+pagedown",
		"command": "cursorBottomSelect",
	},
	// -- navigate source
	{
		"key": "ctrl+f12",
		"command": "editor.action.revealDeclaration",
		"when": "editorTextFocus",
	},
	// -- navigate history
	{
		"key": "ctrl+alt+left",
		"command": "workbench.action.navigateBack",
		"when": "canNavigateBack"
	},
	{
		"key": "ctrl+alt+right",
		"command": "workbench.action.navigateForward",
		"when": "canNavigateForward"
	},
	// -- navigate search
	{
		"key": "alt+]",
		"command": "editor.action.marker.nextInFiles",
		"when": "editorFocus",
	},
	{
		"key": "alt+[",
		"command": "editor.action.marker.prevInFiles",
		"when": "editorFocus",
	},
	{
		"key": "alt+]",
		"command": "search.action.focusNextSearchResult",
		"when": "hasSearchResult || inSearchEditor",
	},
	{
		"key": "alt+[",
		"command": "search.action.focusPreviousSearchResult",
		"when": "hasSearchResult || inSearchEditor",
	},
	{
		"key": "alt+]",
		"command": "references-view.next",
		"when": "reference-list.hasResult && references-view.canNavigate",
	},
	{
		"key": "alt+[",
		"command": "references-view.prev",
		"when": "reference-list.hasResult && references-view.canNavigate",
	},
	{
		"key": "alt+[",
		"command": "goToPreviousReference",
		"when": "inReferenceSearchEditor || referenceSearchVisible",
	},
	{
		"key": "alt+]",
		"command": "goToNextReference",
		"when": "inReferenceSearchEditor || referenceSearchVisible",
	},
	{
		"key": "shift+alt+]",
		"command": "editor.action.marker.next",
		"when": "editorFocus",
	},
	{
		"key": "shift+alt+[",
		"command": "editor.action.marker.prev",
		"when": "editorFocus",
	},
	{
		"key": "shift+alt+]",
		"command": "merge-conflict.next",
		"when": "editorFocus",
	},
	{
		"key": "shift+alt+[",
		"command": "merge-conflict.previous",
		"when": "editorFocus",
	},
	// -- navigate paragraphs
	{
		"key": "alt+up",
		"command": "cursorMove",
		"when": "editorTextFocus",
		"args": {"to": "prevBlankLine"},
	},
	{
		"key": "alt+down",
		"command": "cursorMove",
		"when": "editorTextFocus",
		"args": {"to": "nextBlankLine"},
	},
	{
		"key": "alt+shift+up",
		"command": "cursorMove",
		"when": "editorTextFocus",
		"args": {"to": "prevBlankLine", "select": true},
	},
	{
		"key": "alt+shift+down",
		"command": "cursorMove",
		"when": "editorTextFocus",
		"args": {"to": "nextBlankLine", "select": true},
	},
	// -- navigate words
	//    -- default
	// -- navigate word parts
	{
		"key": "alt+left",
		"command": "cursorWordPartLeft",
		"when": "textInputFocus",
	},
	{
		"key": "alt+right",
		"command": "cursorWordPartRight",
		"when": "textInputFocus",
	},
	{
		"key": "alt+shift+left",
		"command": "cursorWordPartLeftSelect",
		"when": "textInputFocus",
	},
	{
		"key": "alt+shift+right",
		"command": "cursorWordPartRightSelect",
		"when": "textInputFocus",
	},
	// -- navigate brackets
	{
		"key": "ctrl+m",
		"command": "editor.action.jumpToBracket",
		"when": "textInputFocus",
	},
	{
		"key": "ctrl+shift+m",
		"command": "editor.action.selectToBracket",
		"when": "textInputFocus",
	},
	// -- windowing
	{
		"key": "ctrl+alt+\\",
		"command": "workbench.action.focusSideBar",
	},
	{
		"key": "ctrl+alt+s",
		"command": "workbench.action.newGroupRight",
	},
	{
		"key": "ctrl+alt+shift+s",
		"command": "workbench.action.newGroupBelow",
	},
	{
		"key": "ctrl+alt+[",
		"command": "workbench.action.focusPreviousGroup",
	},
	{
		"key": "ctrl+alt+]",
		"command": "workbench.action.focusNextGroup",
	},
	{
		"key": "ctrl+alt+shift+left",
		"command": "workbench.action.moveEditorToPreviousGroup",
	},
	{
		"key": "ctrl+alt+shift+right",
		"command": "workbench.action.moveEditorToNextGroup",
	},
	// -- edit lines
	{
		"key": "ctrl+shift+backspace",
		"command": "deleteAllLeft",
		"when": "editorTextFocus && !editorReadonly",
	},
	{
		"key": "ctrl+shift+delete",
		"command": "deleteAllRight",
		"when": "editorTextFocus && !editorReadonly",
	},
	{
		"key": "ctrl+shift+down",
		"command": "editor.action.moveLinesDownAction",
		"when": "editorTextFocus && !editorReadonly",
	},
	{
		"command": "editor.action.moveLinesUpAction",
		"key": "ctrl+shift+up",
		"when": "editorTextFocus && !editorReadonly",
	},
]
